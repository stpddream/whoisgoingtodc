<li id="<%= 1%>">
	<%= link_to feed_item.user.name, feed_item.user %> was going to <%= feed_item.where %> <%= time_ago_in_words(feed_item.created_at) %> ago.
	<% if current_user?(feed_item.user) %>
	<%= link_to "delete", feed_item, method: :delete, confirm: "you sure?", title: "Delete Post" %>
	<% end %>
</li> 		
