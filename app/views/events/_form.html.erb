<%= form_for(@event) do |f| %>
  <%= f.hidden_field :user_id, :value=>current_user.id %>
  <div class="field">
    <%= f.label :where %><br>
    <%= f.select :where, options_for_select(["No Preference", "Haffner", "Erdman", "Dining Center"]), :class => 'genForm_dropBox' %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
